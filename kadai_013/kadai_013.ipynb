      "source": [
        "# 価格\n",
        "price = 110\n",
        "# 税金\n",
        "tax = 10\n",
        "\n",
        "# 消費税を加えた計算結果を返す関数\n",
        "def calculate_total(price, tax):\n",
        "    # 価格+税金の合計額\n",
        "    total = price + (price * (tax / 100))\n",
        "\n",
        "    # 計算結果を戻り値として返す\n",
        "    return total\n",
        "\n",
        "# 関数を呼び出し、結果を出力する\n",
        "total_price = calculate_total(110, 10)\n",
        "print(f\"{total_price}円\")"
      ]
    }
  ]
}