        "# 上辺：10cm\n",
        "# 下辺：20cm\n",
        "# 高さ：5cm\n",
        "# 面積：area\n",
        "# 公式：台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "# area = (the_top_base + the_bottom_base) * height / 2)\n",
        "\n",
        "the_top_base = 10\n",
        "the_bottom_base = 20\n",
        "height = 5\n",
        "area = ((the_top_base + the_bottom_base) * height / 2)\n",
        "print(str(area) + \"㎠\")"
      ]
